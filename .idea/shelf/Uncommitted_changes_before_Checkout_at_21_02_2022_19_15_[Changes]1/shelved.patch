+import java.lang.*;
+import java.util.*;
+
+public class Dummy {
+    public static final int MAX_HEALTH = 12;
+
+    private final String name;
+    private final int id;
+    public int health;
+
+    public Dummy(String name, int id){
+        this.id = id;
+        this.health = Dummy.MAX_HEALTH;
+        this.name = Objects.requireNonNull(name);
+    }
+
+    @Override
+    public boolean equals(Object o) {
+        if (!(o instanceof Dummy)){
+            return false;
+        }
+
+        Dummy other = (Dummy) o;
+
+        return other.health == this.health && other.name.equals(this.name);
+    }
+
+    @Override
+    public int hashCode() {
+        return name.hashCode() ^ health;
+    }
+
+    public static void main(String[] args) {
+        var dummy1 = new Dummy("timmy", 5);
+        var dummy3 = new Dummy("timmy", 9);
+
+        //Dummy 1 et 3
+        System.out.println(dummy1 == dummy3); // false
+        System.out.println(dummy1.equals(dummy3)); // true
+    }
+}
